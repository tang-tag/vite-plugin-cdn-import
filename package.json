{
  "name": "@cz-soft/vite-plugin-cdn-import",
  "version": "1.0.2",
  "description": "Import packages from CDN for the vite plugin",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "vite cdn plugin",
    "vite CDN extension"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmf-fe/vite-plugin-cdn-import.git"
  },
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts --dts --format esm",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "author": "vfasky<vfasky@me.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "chalk": "^5.6.2",
    "execa": "^9.6.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.6"
  },
  "dependencies": {
    "@pnpm/find-workspace-dir": "^1000.1.3",
    "@pnpm/workspace.find-packages": "^1000.0.48",
    "rollup-plugin-external-globals": "^0.13.0",
    "vite-plugin-externals": "^0.6.2"
  },
  "lint-staged": {
    "*.{ts,js,tsx,json,css,scss,less}": [
      "prettier --write"
    ],
    "*.{scss,less}": [
      "stylelint --fix"
    ]
  }
}
